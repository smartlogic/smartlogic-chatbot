<h1>New Form</h1>

<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :token %><br>
    <%= f.text_field :token %>
  </p>

  <p>
    <%= f.label :team_id %><br>
    <%= f.text_field :team_id %>
  </p>

  <p>
    <%= f.label :team_domain %><br>
    <%= f.text_field :team_domain %>
  </p>

  <p>
    <%= f.label :channel_id %><br>
    <%= f.text_field :channel_id %>
  </p>

  <p>
    <%= f.label :channel_name %><br>
    <%= f.text_field :channel_name %>
  </p>

  <p>
    <%= f.label :timestamp %><br>
    <%= f.text_field :timestamp %>
  </p>

  <p>
    <%= f.label :user_id %><br>
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :trigger_word %><br>
    <%= f.text_field :trigger_word %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
<%= link_to 'Back', articles_path %>

